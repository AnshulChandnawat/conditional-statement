#include<stdio.h>
int main()
{
    int num;
    printf("enter marks: \n");
    scanf("%d",&num);
    if
      (num>=85 && num<=100)
      printf("GRADE=A ");
    else if
      (num>=70 && num<=85)
      printf("GRADE=B ");
    else if
      (num>=55 && num<=70)
       printf("GRADE=C ");
    else if
      (num>=40 && num<=55)
      printf("GRADE=D ");
    else if
      (num>=0 && num<=40)
      printf("GRADE=E ");
    else
      printf("marks range should be 1-100!");
      return 0;
}
